{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "content": {
      "type": "richtext"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "stock": {
      "type": "biginteger"
    },
    "tag": {
      "type": "text"
    },
    "price": {
      "type": "biginteger"
    },
    "priceSale": {
      "type": "biginteger"
    },
    "publish": {
      "type": "boolean"
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "Male",
        "Fmale",
        "Kid"
      ]
    },
    "category_id": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category",
      "private": true
    }
  }
}